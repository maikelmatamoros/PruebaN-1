@model IEnumerable<IF4101SupportApp.Models.Note>


    @if (Model.Count() != 0)
    {
        foreach (var item in Model)
        {
    
            @Html.Partial("_NoteCard", item)
        
        }

    }
    else
    {
    <div class=" row d-flex align-items-center justify-content-center h-100 w-100">
        <span>No notes to show</span>
    </div>
    }
